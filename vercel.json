{
  "crons": [
    {
      "schedule": "* * * * *",
      "path": "/api/v1/indexes/default/reindex"
    },
    {
      "schedule": "* * * * *",
      "path": "/api/v1/sources/tasks/process"
    }
  ],
  "rewrites": [
    {
      "source": "/robots.txt",
      "destination": "https://rag-docs-pingcap.vercel.app/robots.txt"
    },
    {
      "source": "/sitemap(.*)\\.xml",
      "destination": "https://rag-docs-pingcap.vercel.app/sitemap$1.xml"
    },
    {
      "source": "/docs",
      "destination": "https://rag-docs-pingcap.vercel.app/docs"
    },
    {
      "source": "/docs/",
      "destination": "https://rag-docs-pingcap.vercel.app/docs/"
    },
    {
      "source": "/docs/:match*",
      "destination": "https://rag-docs-pingcap.vercel.app/docs/:match*"
    },
    {
      "source": "/blog",
      "destination": "https://rag-docs-pingcap.vercel.app/blog"
    },
    {
      "source": "/blog/",
      "destination": "https://rag-docs-pingcap.vercel.app/blog/"
    },
    {
      "source": "/blog/:match*",
      "destination": "https://rag-docs-pingcap.vercel.app/blog/:match*"
    },
    {
      "source": "/nextra/:match*",
      "destination": "https://rag-docs-pingcap.vercel.app/nextra/:match*"
    }
  ]
}
